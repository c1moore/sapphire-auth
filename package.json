{
  "name": "sapphire-auth",
  "version": "1.1.2",
  "description": "Library for authenticating requests using an OAuth-like signing method.",
  "keywords": [
    "auth",
    "security",
    "oauth",
    "sign",
    "signature",
    "signing",
    "http",
    "request"
  ],
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run build && nyc mocha",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/c1moore/sapphire-auth"
  },
  "bugs": {
    "url": "https://github.com/c1moore/sapphire-auth/issues"
  },
  "author": "Calvin Moore <c1moore@outlook.com>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/express": "^4.17.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "should": "^13.2.3",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "reporter": [
      "json",
      "lcov",
      "text"
    ],
    "exclude": [
      "**/test/**",
      "**/src/**",
      "**/node_modules/**",
      "**/coverage/**"
    ],
    "excludeAfterRemap": false,
    "statements": 90,
    "branches": 90
  }
}
